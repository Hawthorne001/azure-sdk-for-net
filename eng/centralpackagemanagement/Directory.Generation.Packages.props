    <UnbrandedGeneratorVersion>1.0.0-alpha.20260224.5</UnbrandedGeneratorVersion>
    <AzureGeneratorVersion>1.0.0-alpha.20260219.1</AzureGeneratorVersion>
  </PropertyGroup>

  <!-- Packages used by the TypeSpec generators -->
